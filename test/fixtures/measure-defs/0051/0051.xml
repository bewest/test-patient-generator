   </subjectOf>
   <subjectOf>
      <measureAttribute>
         <code code="DEF" codeSystem="2.16.840.1.113883.3.560" displayName="Definition"/>
         <value xsi:type="ED" mediaType="text/plain">Numerator(s): Patient visits with assessment for use of anti-inflammatory or analgesic OTC medications</value>
      </measureAttribute>
   </subjectOf>
   <subjectOf>
      <measureAttribute>
         <code code="DEF" codeSystem="2.16.840.1.113883.3.560" displayName="Definition"/>

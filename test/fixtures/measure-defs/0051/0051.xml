         <code code="DEF" codeSystem="2.16.840.1.113883.3.560" displayName="Definition"/>
         <value xsi:type="ED" mediaType="text/plain">Denominator(s): All patient visits for patients aged 21 years and older with a diagnosis of Osteoarthritis</value>
      </measureAttribute>
   </subjectOf>
   <subjectOf>
      <measureAttribute>
         <code code="DEF" codeSystem="2.16.840.1.113883.3.560" displayName="Definition"/>

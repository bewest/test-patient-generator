      <section>
         <code code="57026-7" codeSystem="2.16.840.1.113883.6.1" displayName="Population criteria"/>
         <title>Population criteria</title>
         <text>
            <list>
               <item>
                  <content styleCode="Bold">Initial Patient Population =</content>
                  <list>
                     <item>AND: <list>
                           <item>OR: "Encounter: Encounter Office &amp; Outpatient Consult"</item>
                           <item>OR: "Encounter: Encounter Psychiatric &amp; Psychologic"</item>
                           <item> during "Measurement period"</item>
                        </list>
                     </item>
                     <item>AND: "Patient Characteristic: birth date"  &gt;= 18 year(s) starts before start of "Measurement period"</item>
                     <item>AND: <list>
                           <item>AND: "Diagnosis, Active: Major Depressive Disorder New or Recurrent" starts before or during <list>
                                 <item>OR: "Encounter: Encounter Office &amp; Outpatient Consult"</item>
                                 <item>OR: "Encounter: Encounter Psychiatric &amp; Psychologic"</item>
                                 <item> during "Measurement period"</item>
                              </list>
                           </item>
                           <item>AND: Count &gt;= 2  of:<list>
                                 <item>OR: "Encounter: Encounter Office &amp; Outpatient Consult"</item>
                                 <item>OR: "Encounter: Encounter Psychiatric &amp; Psychologic"</item>
                                 <item> during "Measurement period"</item>
                              </list>
                           </item>
                        </list>
                     </item>
                  </list>
               </item>
               <item>
                  <content styleCode="Bold">Denominator =</content>
                  <list>
                     <item>AND: "Initial Patient Population"</item>
                  </list>
               </item>
               <item>
                  <content styleCode="Bold">Denominator Exclusions =</content>
                  <list>
                     <item>None</item>
                  </list>
               </item>
               <item>
                  <content styleCode="Bold">Numerator =</content>
                  <list>
                     <item>AND: <list>
                           <item>OR: "Procedure, Performed: Suicide Risk Assessment"</item>
                           <item>OR: "Symptom, Assessed: Suicide Risk Findings"</item>
                           <item>OR: "Risk Category Assessment: Suicide Risk Scale (result)"</item>
                           <item> during <list>
                                 <item>OR: "Encounter: Encounter Office &amp; Outpatient Consult"</item>
                                 <item>OR: "Encounter: Encounter Psychiatric &amp; Psychologic"</item>
                                 <item> during "Measurement period"</item>
                              </list>
                           </item>
                        </list>
                     </item>
                  </list>
               </item>
               <item>
                  <content styleCode="Bold">Denominator Exceptions =</content>
                  <list>
                     <item>None</item>
                  </list>
               </item>
            </list>

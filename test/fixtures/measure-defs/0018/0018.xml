      <section>
         <code code="57026-7" codeSystem="2.16.840.1.113883.6.1" displayName="Population criteria"/>
         <title>Population criteria</title>
         <text>
            <list>
               <item>
                  <content styleCode="Bold">Initial Patient Population =</content>
                  <list>
                     <item>AND: "Patient Characteristic: birth date"  &gt;= 17 year(s) starts before start of "Measurement period"</item>
                     <item>AND: "Patient Characteristic: birth date"  &lt;= 84 year(s) starts before start of "Measurement period"</item>
                  </list>
               </item>
               <item>
                  <content styleCode="Bold">Denominator =</content>
                  <list>
                     <item>AND: "Initial Patient Population"</item>
                     <item>AND: "Diagnosis, Active: hypertension"  &lt;= 6 month(s) starts after start of "Measurement period"</item>
                     <item>AND: "Encounter: Encounter outpatient" during "Measurement period"</item>
                  </list>
               </item>
               <item>
                  <content styleCode="Bold">Denominator Exclusions =</content>
                  <list>
                     <item>AND: <list>
                           <item>OR: "Diagnosis, Active: ESRD"</item>
                           <item>OR: "Procedure, Performed: Procedures indicative of ESRD"</item>
                           <item>OR: "Diagnosis, Active: pregnancy"</item>
                           <item> during "Measurement period"</item>
                        </list>
                     </item>
                  </list>
               </item>
               <item>
                  <content styleCode="Bold">Numerator =</content>
                  <list>
                     <item>AND: <list>
                           <item>AND: Minimum of:"Physical Exam, Finding: diastolic blood pressure (result &lt; 90 mmHg)"</item>
                           <item>AND: Minimum of:"Physical Exam, Finding: systolic blood pressure (result &lt; 140 mmHg)"</item>
                           <item> during  (MOST RECENT:"Encounter: Encounter outpatient" during "Measurement period")</item>
                        </list>
                     </item>
                  </list>
               </item>
               <item>
                  <content styleCode="Bold">Denominator Exceptions =</content>
                  <list>
                     <item>None</item>
                  </list>
               </item>
            </list>
